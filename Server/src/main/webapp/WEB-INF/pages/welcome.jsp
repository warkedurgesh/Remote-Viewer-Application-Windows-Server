<%@ page language="java" contentType="text/html; charset=ISO-8859-1"         pageEncoding="ISO-8859-1" %><!DOCTYPE html><html><style>    html,body{        margin:0;        height:fit-content;    }    img{        display:block;        width:100%; height:100%;        object-fit: contain;    }    * {        margin: 0;        padding: 0;        box-sizing: border-box;    }    .wrapper {        display: grid;        grid-template-columns: repeat(${gridValue}, 1fr);        grid-template-rows: repeat(${gridValue}, 1fr);        grid-auto-rows: 100px;        grid-gap: 0px;    }    .grid-item {        border: 0px solid rgba(0, 0, 0, 0);        padding: 0px;    }</style><head>    <meta charset="ISO-8859-1">    <title>Phantom</title></head><body><script> setTimeout(function() {        location.reload();    }, 500); </script><div class="wrapper" id="HTMLWrapper"></div>    <script>        var wrapper = document.getElementById("HTMLWrapper");        var i;        var HTML ='';        var cars = ["${screen_1}","${screen_2}","${screen_3}","${screen_4}","${screen_5}","${screen_6}","${screen_7}","${screen_8}","${screen_9}","${screen_10}","${screen_11}","${screen_12}","${screen_13}","${screen_14}","${screen_15}","${screen_16}","${screen_17}","${screen_18}","${screen_19}","${screen_20}","${screen_21}","${screen_22}","${screen_23}","${screen_24}","${screen_25}"];        for (i=0; i<${count}; i++) {            HTML += '<div><img src="data:image/jpeg;base64,x"/></div>'.replace("x",cars[i]);        }        wrapper.innerHTML = HTML    </script></body></html>